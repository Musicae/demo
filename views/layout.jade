doctype html
html
  head
    title= title
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous")
    block stylesheets
      link(rel='stylesheet', href='/stylesheets/style.css')
  body
    nav.navbar
      div.navbar-header
        button.navbar-toggle.collapsed(type="button" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false")
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href="#")
          img(src="/images/Musicae_Black.png" alt="Musicae Logo" style="max-height: 100%; padding: 5px;")
      div.collapse.navbar-collapse(id="bs-example-navbar-collapse-1")
        ul.nav.navbar-nav
          li.dropdown
            a.dropdown-toggle(href="#" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false") Composers
            ul.dropdown-menu
              li: a(href="#") Action
              li: a(href="#") Another action
              li: a(href="#") Something else here
              li.divider(role="separator")
              li: a(href="#") Separated link
              li.divider(role="separator")
              li: a(href="#") One more separated link
          li.dropdown
            a.dropdown-toggle(href="#" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false") Perfomers
            ul.dropdown-menu
              li: a(href="#") Action
              li: a(href="#") Another action
              li: a(href="#") Something else here
              li.divider(role="separator")
              li: a(href="#") Separated link
              li.divider(role="separator")
              li: a(href="#") One more separated link
          li.dropdown
            a.dropdown-toggle(href="#" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false") Genres
            ul.dropdown-menu
              li: a(href="#") Action
              li: a(href="#") Another action
              li: a(href="#") Something else here
              li.divider(role="separator")
              li: a(href="#") Separated link
              li.divider(role="separator")
              li: a(href="#") One more separated link
        div.navbar-right#sign-in-div
          ul.nav.navbar-nav
            li: a(href="#") Sign in / Register
        form.navbar-form.navbar-right
          div.form-group
            input.form-control(type="text" placeholder="Search")
    block content
    script(src="https://code.jquery.com/jquery-3.1.0.min.js"   integrity="sha256-cCueBR6CsyA4/9szpPfrX3s49M9vUU5BgtiJj06wt/s="   crossorigin="anonymous")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous")
    block scripts
